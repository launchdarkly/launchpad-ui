{
  "$schema": "./node_modules/syncpack/schema.json",
  "sortPackages": true,
  "strict": true,
  "versionGroups": [
    {
      "label": "Enforce workspace protocol for workspace packages",
      "dependencies": [
        "@launchpad-ui/**"
      ],
      "specifierTypes": [
        "workspace-protocol"
      ]
    },
    {
      "dependencyTypes": [
        "peer",
        "dev",
        "prod"
      ],
      "label": "Peer deps must be up to date",
      "dependencies": [
        "*"
      ],
      "specifierTypes": [
        "exact"
      ],
      "preferVersion": "highestSemver"
    }
  ],
  "sortFirst": [
    "name",
    "private",
    "version",
    "description",
    "repository",
    "author",
    "bugs",
    "homepage",
    "license",
    "status",
    "workspaces",
    "main",
    "types",
    "module",
    "source",
    "files",
    "publishConfig",
    "sideEffects",
    "scripts",
    "keywords",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "browserslist",
    "engineStrict",
    "engines",
    "resolutions",
    "packageManager",
    "pnpm"
  ]
}