{
  "name": "@launchpad-ui/icons",
  "version": "0.2.4",
  "status": "beta",
  "publishConfig": {
    "access": "public"
  },
  "description": "An element that supplements content and represents an action or feature within LaunchDarkly.",
  "files": [
    "dist"
  ],
  "style": "dist/index.css",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./styles/*": "./dist/styles/*"
  },
  "source": "src/index.ts",
  "scripts": {
    "build": "pnpm build:icons && tsc --project tsconfig.build.json && node ../../scripts/build.js",
    "build:icons": "svgr --silent -- icons",
    "clean": "rm -rf dist",
    "e2e": "playwright test --config=../../playwright.config.ct.ts",
    "lint": "eslint '**/*.{ts,tsx,js}' && stylelint '**/*.css' --ignore-path ../../.stylelintignore",
    "test": "pnpm build:icons && vitest run --coverage"
  },
  "dependencies": {
    "@launchpad-ui/tokens": "workspace:~",
    "clsx": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.3.1",
    "@types/uuid": "^8.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
