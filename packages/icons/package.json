{
  "name": "@launchpad-ui/icons",
  "version": "0.8.8",
  "status": "beta",
  "publishConfig": {
    "access": "public"
  },
  "description": "An element that supplements content and represents an action or feature within LaunchDarkly.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css"
  },
  "source": "src/index.ts",
  "scripts": {
    "build": "pnpm build:types && vite build -c ../../vite.config.ts && tsc --project tsconfig.build.json && pnpm build:sprite",
    "build:sprite": "rm -rf dist/sprite.svg && node sprite.js && prettier -w --parser html dist/sprite.svg",
    "build:types": "node types.js && prettier -w src/types.ts",
    "clean": "rm -rf dist",
    "lint": "eslint '**/*.{ts,tsx,js}' && stylelint '**/*.css' --ignore-path ../../.stylelintignore",
    "test": "pnpm build:icons && vitest run --coverage"
  },
  "dependencies": {
    "@launchpad-ui/tokens": "workspace:~",
    "classix": "2.1.17"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
